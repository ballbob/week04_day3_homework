<h1>Recently registered students</h1>

<% @students.each do |student| %>
  
  <p class="student">First name: <%= student.first_name %> <%= student.last_name %></p>
  <p class="student">Surame: <%= student.last_name %></p>
  <p class="student">House: <%= student.house %></p>
  <p class="student">Age: <%= student.age %></p>
  <hr />
<% end %>